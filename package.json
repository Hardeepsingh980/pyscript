{
    "name": "pyscript",
    "displayName": "pyscript",
    "description": "Code Highlighter for PyScript",
    "publisher": "HardeepSingh",
    "icon": "images/logo.png",
    "galleryBanner": {
        "color": "#0c4b33",
        "theme": "dark"
    },
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "recommendations": [
        "ms-python.python"
    ],
    "keywords": [
        "python",
        "django",
        "web",
        "pyscript",
        "django templates",
        "anaconda"
    ],
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Hardeepsingh980/pyscript.git"
    },
    "homepage": "https://github.com/Hardeepsingh980/pyscript.git",
    "bugs": {
        "url": "https://github.com/Hardeepsingh980/pyscript.git/issues"
    },
    "contributes": {
        "languages": [{
            "id": "pyscript",
            "aliases": ["pyscript", "pyscript"],
            "extensions": [".html"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pyscript",
            "scopeName": "text.html.pyscript",
            "path": "./syntaxes/pyscript.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "pyscript",
                "scopeName": "text.html.pyscript",
                "path": "./snippets/pyscript-snippets.json"
            }
        ]
    }
}